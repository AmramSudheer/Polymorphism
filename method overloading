class Test:
    def sum (self,a = None,b = None,c = None):
        if a!=None and b!=None and c!= None:
            print("sum of 3 numbers is:",a+b+c)
        elif a!=None and b!= None:
            print("sum of two numbers is:",a+b)

        else:
            print("enter 2 or 3 numbers")
t = Test()
t.sum(10,20,30)
print(t.sum)
t.sum(40,60)
print(t.sum)
t.sum(12)
print(t.sum)
